{
    "name": "fm-labs/cakephp-cupcake",
    "description": "Cupcake extension for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "fm-labs/cakephp-devtools": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Cupcake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cupcake\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
