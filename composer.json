{
    "name": "fm-labs/cakephp-cupcake",
    "description": "Cupcake extension for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=8.2",
        "ext-pdo": "*",
        "cakephp/cakephp": "^5"
    },
    "require-dev": {},
    "suggest": {
        "fm-labs/cakephp-devtools": "CakePHP developer toolset",
        "fm-labs/cakephp-essentials": "CakePHP core essentials",
        "fm-labs/cakephp-settings": "Settings plugin for CakePHP"
    },
    "autoload": {
        "psr-4": {
            "Cupcake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "Cupcake\\Test\\": "tests"
        }
    }
}
